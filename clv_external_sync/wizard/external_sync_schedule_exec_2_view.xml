<odoo>

    <record id="clv_external_sync_schedule_exec_2_view_form" model="ir.ui.view">
        <field name="name">clv.external_sync.schedule.exec_2.form</field>
        <field name="model">clv.external_sync.schedule.exec_2</field>
        <field name="arch" type="xml">
            <form>
                <div class="oe_right">
                </div>
                <label for="external_sync_batch_member_ids" class="oe_edit_only"/>
                <field name="external_sync_batch_member_ids">
                    <!--tree>
                        <field name="name"/>
                        <field name="code"/>
                        <field name="age"/>
                        <field name="category_ids"/>
                        <field name="address_id" string="Current Address"/>
                    </tree-->
                </field>
                <group>
                    <group>
                        <field name="count_schedules"/>
                    </group>
                </group>
                <footer>
                    <button 
                        type="object"
                        name="do_external_sync_schedule_exec_2"
                        string="External Sync Schedule Exec (2)"
                        class="oe_highlight"
                        />
                    <button special="cancel" string="Cancel"/>
                </footer>
            </form>

        </field>
    </record>

    <!-- More button Action -->
    <act_window 
        id="clv_external_sync_schedule_exec_2_action"
        name="External Sync Schedule Exec (2)"
        binding_model="clv.external_sync.batch.member"
        res_model="clv.external_sync.schedule.exec_2"
        view_mode="form"
        target="new"
    />

</odoo>
